{
  "version": 2,
  "builds": [
    {
      "src": "dist/proagro/server/server.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/proagro/browser/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff2?|ttf|eot))",
      "dest": "/dist/proagro/browser/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/proagro/server/server.mjs"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
