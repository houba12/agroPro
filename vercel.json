{
  "version": 2,
  "builds": [
    {
      "src": "dist/proagro/server/server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/proagro/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/proagro/browser/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff2?|ttf|eot))",
      "dest": "/dist/proagro/browser/$1"
    },
    {
      "src": "^/(api|server/.*)",
      "dest": "/dist/proagro/server/server.mjs"
    },
    {
      "src": "/.*",
      "dest": "/dist/proagro/browser/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
